<% @tabs = 0 -%>
<% make_module_parents[0, make_module_parents.size - 1].each do |mod| -%>
<%= tabs + 'module ' + mod %><% @tabs+=1 %>
<% end -%>
<%= tabs %>class <%= make_module_parents.last.camelize %> < <%= make_module_parents[make_module_parents.size - 2].camelize %>
<%= tabs %>  def initialize
<%= tabs %>    super do
<%= tabs %>      { '<%= make_module_parents.last.underscore %>' => <%= 'QbMap::' + make_module_parents.map{|iter| iter.camelize}.join('::') %> }
<%= tabs %>    end
<%= tabs %>  end
<%= tabs %>end
<% make_module_parents[0, make_module_parents.size - 1].each do -%>
<% @tabs-=1 -%><%= tabs + 'end' %>
<% end %>